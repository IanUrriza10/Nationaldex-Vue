{
	"root": true,
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"parser": "@typescript-eslint/parser"
	},
	"extends": [
		"plugin:vue/strongly-recommended",
		"eslint:recommended",
		"prettier"
	],
	"plugins": ["@typescript-eslint", "prettier"],
	"rules": {
		"prettier/prettier": "error",
		"no-undef": "warn"
	},
	"ignorePatterns": [
		"shims-vue.d.ts",
		"vite.config.ts",
		"main.ts",
		"./utils/Graphql/*",
		"./utils/generated/*"
	]
}
